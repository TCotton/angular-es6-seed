{
  "name": "angular-es6-seed",
  "version": "0.1.0",
  "description": "Angular ES6 seed -  Gulp, JSPM, System.js, ES6 modules, SASS and SUSY",
  "author": "Andy Walpole",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TCotton/angular-es6-seed/issues"
  },
  "homepage": "https://github.com/TCotton/angular-es6-seed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCotton/angular-es6-seed.git"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "",
    "security": "snyk test",
    "bump": "bump --prompt",
    "modernizr": "modernizr -c modernizr-config.json -d ./src/assets/vendor/modernizr.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "install": "npm install && jspm install && jspm dl-loader && gulp constants && gulp sassDev && npm run modernizr",
    "clean": "rm -r node_modules && rm -r src/assets/vendor/ && rm -r src/jspm_packages && rm -r src/app/config && rm -r src/assets/images-optim",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "bundle": "jspm bundle app/app.js dist/app/app.bundle.js",
    "bundleInject": "jspm bundle app/app.js dist/app/app.bundle.js --inject",
    "unbundle": "jspm unbundle",
    "bundleMinify": "jspm bundle app/app.js dist/app/app.bundle.js --inject --minify",
    "bundleMinifyAlt": "jspm bundle app/app.js dist/app/app.bundle.js --inject --minify --no-mangle",
    "bundleSFX": "jspm bundle-sfx app/app.js dist/app/app.bundle.js --inject",
    "bundleSFXMinify": "jspm bundle-sfx app/ app.js dist/app/app.bundle.js --inject --minify",
    "bundleSFXMinifyAlt": "jspm bundle-sfx app/app.js dist/app/app.bundle.js --inject --minify --no-mangle",
    "depcache": "jspm depcache app/app.js jspm install",
    "server": "jspm-server src --only-exts='.jsx'"
  },
  "keywords": [
    "angular",
    "jspm",
    "system.JS",
    "systemjs"
  ],
  "dependencies": {},
  "devDependencies": {
    "api-check": "^7.5.5",
    "astrolabe": "^0.3.6",
    "babel-cli": "^6.3.17",
    "commitizen": "^2.4.6",
    "conventional-changelog": "^0.5.1",
    "css-mqpacker": "^4.0.0",
    "csswring": "^4.1.1",
    "cz-conventional-changelog": "^1.1.5",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-cache": "^0.4.1",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-conventional-changelog": "^0.7.0",
    "gulp-cssnano": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jscs": "^3.0.2",
    "gulp-jshint": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.1.0",
    "gulp-modernizr": "^1.0.0-alpha",
    "gulp-newer": "^1.1.0",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-ng-constant": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-nsp": "^2.2.1",
    "gulp-open": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-posthtml": "^1.5.2",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^6.0.1",
    "gulp-sass": "^2.1.1",
    "gulp-scss-lint": "^0.3.8",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svgmin": "^1.2.0",
    "gulp-uglify": "^1.5.1",
    "imagemin": "^4.0.0",
    "imagemin-mozjpeg": "^5.1.0",
    "imagemin-pngquant": "^4.2.0",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.8.0",
    "jspm": "0.16.19",
    "jspm-server": "^1.0.1",
    "karma": "^0.13.16",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.2",
    "karma-junit-reporter": "^0.3.8",
    "modernizr": "^3.2.0",
    "ng-describe": "^1.6.0",
    "postcss-fakeid": "^1.0.2",
    "sass-lint": "^1.4.0",
    "snyk": "^1.8.2",
    "susy": "^2.2.9",
    "version-bump-prompt": "^1.5.2"
  },
  "engines": {
    "node": ">=0.4.0",
    "npm": ">=3.5.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "configFile": "src/jspm-config.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.8",
      "angular-aria": "github:angular/bower-angular-aria@^1.4.8",
      "angular-deferred-bootstrap": "github:philippd/bower-angular-deferred-bootstrap@^0.1.9",
      "angular-formly": "github:formly-js/angular-formly@^7.3.9",
      "angular-mocks": "github:angular/bower-angular-mocks@^1.4.8",
      "angular-resource": "github:angular/bower-angular-resource@^1.4.8",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.4.8",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15",
      "angular-ui-router-extras": "github:christopherthielen/ui-router-extras@^0.1.0",
      "api-check": "npm:api-check@^7.5.5",
      "babel-polyfill": "npm:babel-polyfill@^6.3.14",
      "babel-preset-es2015": "npm:babel-preset-es2015@^6.3.13",
      "core-js": "npm:core-js@^1.1.4",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "image": "github:systemjs/plugin-image@^0.1.0",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "lodash": "npm:lodash@^3.10.1",
      "moment": "npm:moment@^2.10.6",
      "text": "github:systemjs/plugin-text@^0.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "clean-css": "npm:clean-css@^3.4.8"
    }
  }
}
